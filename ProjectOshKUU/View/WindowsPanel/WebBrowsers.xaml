<Window x:Class="ProjectOshKUU.View.WindowsPanel.WebBrowsers"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectOshKUU.View.WindowsPanel"
        mc:Ignorable="d"
        Icon="/Image/FastAccess/maps-and-flags.png"
        Loaded="Window_Loaded" ShowInTaskbar="False" WindowState="Normal" WindowStartupLocation="CenterScreen"  Height="850" Width="1400">
    <Grid>
        <Button Panel.ZIndex="1"  Margin="50" Style="{DynamicResource ButtonClose}" Uid="Закрыть" Height="50" Width="150"/>
        <WebBrowser  x:Name="Browser" Grid.Row="0"  ></WebBrowser>
        

    </Grid>
</Window>
